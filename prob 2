// PLEASE PRESS ENTER AFTER ENTER 1 CHARACTER


#include <iostream>

using namespace std;

int main()
{
    int swapval;
    int x;
    int l;
	
	int arr[x];
	
	cout<<"Enter array size: ";cin>>x;
	cout<<"Enter array values"<<endl;
	for(int i = 0; i < x; i++)
	{
	    cin>>arr[i];
	}
	for(int c = l - 1; c > 0; c--)
	{
	    for(int i = 0; i < x; i++)
	    {
	        if(arr[i] > arr[i + 1])
	        {
	            swapval = arr[i + 1];
	            arr[i + 1] = arr[i];
	            arr[i] = swapval;
	        }
	    }
	    
	}
	
	for(int i = 0; i < x; i++)
	    {
	        cout<<arr[i]<<", ";
	    }
	    
	return 0;

}
